#include <stdio.h>

int main() {
    int num = 24;

    printf("Enter a number to check if even or odd: ");
    scanf("%d", &num);

    if (num % 2 == 0)
        printf("%d is even.\n", num);
    else
        printf("%d is odd.\n", num);

    return 0;
}