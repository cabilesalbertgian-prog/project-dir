#include <stdio.h>

int main() {
    int i;

    printf("While Loop (1 to 10):\n");
    i = 1;
    while (i <= 10) {
        printf("%d ", i);
        i++;
    }
    printf("\n\n");

    printf("For Loop (Even Numbers 2 to 20):\n");
    for (i = 2; i <= 20; i += 2) {
        printf("%d ", i);
    }
    printf("\n\n");

    printf("Do-While Loop (1 to 5):\n");
    i = 1;
    do {
        printf("%d ", i);
        i++;
    } while (i <= 5);

    return 0;
}

